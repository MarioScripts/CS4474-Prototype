{
  "name": "CS4474_Prototype",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "description": "Prototype for CS4474",
  "build": {
    "appId": "com.cs4474.prototype",
    "files": [
      "dist/**/*",
      "public/**/**",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null,
    "nsis": {
      "artifactName": "${productName}_Setup_${version}.${ext}"
    }
  },
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "cross-env": "^7.0.2",
    "electron-context-menu": "^2.3.0",
    "electron-is-dev": "^1.2.0",
    "electron-settings": "^4.0.2",
    "lodash.isequal": "^4.5.0",
    "music-metadata": "^7.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-player": "^2.6.2",
    "util": "^0.12.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "webpack-build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "electron-build": "electron-builder",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run webpack-build && npm run electron-build",
    "start": "concurrently --kill-others --success first \"cross-env BROWSER=none NODE_ENV=development webpack serve --config config/webpack.prod.js\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@svgr/webpack": "^5.4.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "file-loader": "^6.1.1",
    "hot-module-replacement": "^3.0.4",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.2.1",
    "node-sass": "^4.14.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "wait-on": "^5.2.0",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-copy-plugin": "0.0.4",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.2.0"
  }
}
